    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "windows": {
                "program": "${workspaceFolder}/cygwin/jvm",
                "MIMode": "gdb",
                "miDebuggerPath": "C:/cygwin64/bin/gdb.exe",
                // "sourceFileMap": {
                //     "/cygdrive/c/C++Projects/jvm-1/src": "/cygdrive/c/C++Projects/jvm-1/src"
                // }
            },
            "linux": {
                "MIMode": "gdb",
                "miDebuggerPath": "usr/bin/gdb",
            },
            "osx": {
                "MIMode": "lldb",
                "program": "${workspaceFolder}/cygwin/jvm",
            },
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}